{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/singleliner",
        "dest": "index.js"
      },
      {
        "src": "/api/singleliner/:id",
        "dest": "index.js"
      },
      {
        "src": "/api/jokes",
        "dest": "index.js"
      },
      {
        "src": "/api/jokes/:id",
        "dest": "index.js"
      },
      {
        "src": "/api/random",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
}